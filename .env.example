# Interactive Brokers Credentials
IB_USERNAME=your_ib_username
IB_PASSWORD=your_ib_password
TRADING_MODE=paper

# TOTP Authentication (for headless operation)
# Get this from IB when enabling 2FA - format: XXXXXXXXXXXXXXXXXXXX (20 characters)
IB_TOTP_SECRET=your_totp_secret_here

# Trading Bot Configuration
PAPER_TRADING=true
GAP_THRESHOLD=3.0
MAX_POSITION_SIZE=1000
MAX_POSITIONS=5

# IB Gateway Connection (usually no need to change these)
IB_HOST=ibgateway
IB_PORT=4001
IB_CLIENT_ID=1
